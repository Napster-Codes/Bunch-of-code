import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns
# loading dataset 
data = sns.load_dataset("iris") 
  
  
def plot():
    sns.lineplot(x="sepal_length", y="sepal_width", data=data) 
  
      
    # Adding the subplot
    plt.subplot(211)
    plot()
      
plt.subplot(212)
plot()
